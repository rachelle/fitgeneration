<div class="workout-banner">

  <div class="container">
    <div class="row">
      <div class="col s12 m12 l12">
        <div class="exercise-content">
        <h3 class="workout-title center-align">Customize Plan</h3> 
   
        <!-- workout form -->
        <form action="/workouts" method="post" id="workout-form" >
          <br/>
          <input type="text" name="name" placeholder="name"> 
          <br/>
          <input type="text" name="sets" placeholder="sets">
          <input type="text" name="reps" placeholder="reps">
          <input type="text" name="weight"
          placeholder="weight">

          <h6>Body Part</h6>
    
          <select class="browser-default" name="bodypart">
            <option value="" disabled selected>select option</option>
            <option value="1">legs</option>
            <option value="2">glutes</option>
            <option value="3">abs</option>
          </select>
    
          <button type="text" name="Submit" class="action-button" value="Submit">Create</button>
        </form>
        <!-- end of workout form -->
      </div>
    </div>
  </div>
</div>


